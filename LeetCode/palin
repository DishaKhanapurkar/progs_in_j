class Solution {
    public boolean isPalindrome(int x) {
        String numstr=Integer.toString(x);
        String ch="";
        char nstr;
        for(int i=0;i<numstr.length();i++){
            nstr=numstr.charAt(i);
            ch=nstr+ch;
        }
        if(ch.equals(numstr))
              return true;
        
        else
            return false;
    }
}
